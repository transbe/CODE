        body {
            display: -webkit-flex;
            /* Safari */
            display: flex;
            flex-wrap: nowrap;
            overflow-y: hidden;
        }
        /*---------------左侧区域样式----------------开始----------------------*/
        
        .div-left {
            width: 20%;
            height: 100%;
            overflow: hidden;
            background-color: #fff;
            flex-shrink: 0
        }
        
        .div-left-header {
            background-color: rgb(231, 232, 236);
            height: 38px;
            min-width: 164px;
        }
        
        .div-left-header ul {
            position: relative;
            margin-top: 0;
            margin-bottom: -30px;
            padding-top: 9px;
            padding-left: 10px;
        }
        
        .div-left-header ul a {
            margin-left: 33%;
        }
        
        .div-left-header ul a:hover {
            cursor: pointer;
        }
        
        .div-left-header ul span {
            position: absolute;
            right: 10px;
            top: 8px;
        }
        /*左侧树样式*/
        
        .treeview-box {
            min-width: 230px;
            overflow: auto;
            box-sizing: content-box;
        }
        
        .treeview-box input {
            color: #000;
        }
        
        .treeview {
            width: 100%;
            height: 100%;
        }
        
        .vakata-context {
            z-index: 10002;
        }
        /*---------------左侧区域样式----------------结束----------------------*/
        /*---------------中间分割区域样式----------------开始----------------------*/
        
        .divider {
            width: 8px;
            height: 100%;
            background-color: #f3f3f3;
            border-right: 1px solid #b7b5b7;
            z-index: 10001;
        }
        
        .divider:hover {
            cursor: e-resize;
        }
        
        .divider i {
            padding-top: 10px;
            color: #58b6fc;
        }
        
        .divider i:hover {
            cursor: pointer;
        }
        
        .ishide i {
            transform: rotate(180deg);
        }
        /*---------------中间分割区域样式----------------结束----------------------*/
        /*---------------右侧区域样式----------------开始----------------------*/
        
        .div-right {
            width: 80%;
            height: 100%;
            background-color: #fff;
        }
        /* 绘图页面的table设备列表  */
        
        #equipmentlist {
            position: fixed;
            left: 123px;
            top: 110px;
            border: 1px solid #CCCCCC;
            min-width: 200px;
            max-width: 700px;
            background-color: #fff;
        }
        
        #equipmentheader {
            height: 30px;
            background-color: #A8A8A8;
            text-align: left;
            padding-left: 10px;
            font-weight: bold;
            cursor: pointer;
        }
        
        #equipmentlist tr {
            text-align: center;
        }
        
        #equipmentlist .head-bg {
            background-color: #edf1f2;
            /*color:#fff;*/
        }
        
        #chartDraw {
            padding: 0;
            margin: 0;
            border: 0px;
        }
        /*控制属性显隐样式*/
        
        .attr-show,
        copy-segment, 
        paste-segment{
            cursor: pointer;
        }
        
        .attr-show:hover {
            background: url("../../html/segment/svg/icon/shuxing_02.png");
        }
        copy-segment:hover{
            background: url("../../html/segment/svg/icon/copy_03.png");
        }
        paste-segment:hover{
            background: url("../../html/segment/svg/icon/paste_03.png");
        }
        #extract img:hover {
            background: url("../../html/segment/svg/icon/chouqu_02.png");
            cursor: pointer;
        }
        
        #publish img:hover {
            background: url("../../html/segment/svg/icon/fabu_02.png");
            cursor: pointer;
        }
        
        #edit img:hover {
            background: url("../../html/segment/svg/icon/bianji_02.png");
            cursor: pointer;
        }
        /*工具栏分割竖线*/
        
        .tool-divider {
            width: 1px;
            border-left: 1px solid #b7b5b7;
        }
        
        .whitespace {
            padding: 5px;
        }
        /*---------------右侧区域样式----------------结束----------------------*/
