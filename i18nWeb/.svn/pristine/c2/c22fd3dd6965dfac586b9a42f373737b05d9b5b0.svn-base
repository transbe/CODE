        autoPlay: true,
        autoPage: true,
        trigger: "click",
        mouseOverStop: true,
        easing: "swing"
    });
    $(function() {
        //根据滚动条判断右下角的图片显示或隐藏
        //加载
        lsObj.setLocalStorage('i18nLanguage', "en");
        lsObj.setLocalStorage('timeOut', new Date().getTime() + (10 * 60 * 60 * 1000));
        $(window).scroll(function() {
            var scrollTop = $(window).scrollTop();
            if (scrollTop > 1024) {
                $('.to_top').css({
                    display: 'block'
                })
            } else {
                $('.to_top').css({
                    display: 'none'
                })
            }
        });

    });
    $(".switch").click(function(){
        lsObj.setLocalStorage('i18nLanguage', "zh");
        lsObj.setLocalStorage('timeOut', new Date().getTime() + (10 * 60 * 60 * 1000));
    })
       
    
</script>
