 /*通用样式*/
 
 body {
     background: #edf1f2;
 }
 
 ul,
 li {
     list-style: none;
     padding: 0;
     margin: 0;
 }
 
 a {
     text-decoration: none;
 }
 
 .flex-box {
     display: -webkit-flex;
     display: flex;
     flex-wrap: nowrap;
 }
 
 .welcom-box {
     padding: 8px;
 }
 
 .welcom-header {
     width: 100%;
     line-height: 40px;
     padding: 10px;
     background: #fff;
     flex-wrap: nowrap;
     justify-content: space-between;
     border-radius: 4px;
 }
 
 .header-model {
     flex-basis: 24%;
     border-radius: 4px;
     padding: 0 10px;
     color: #fff;
     background: #59b6fc;
     /*border: 1px solid red*/
 }


 /*内容部分*/
 
 .content-main {
     align-content: center;
     justify-content: space-between;
     margin-top: 10px;
     flex-wrap: nowrap;
     min-height: 500px;
 }
 .content-main .main-left{
     flex-basis: 79.5%;
     /*background-color: skyblue;*/
     align-content: space-between;
     /*flex-wrap: wrap;*/
     flex-direction: column;
 }
  .content-main .main-right{
     flex-basis: 20%;
     background-color: #fff;
     padding: 5px;
 }

 /*左边 区域上方部分*/
.main-left .left-box-top{
    flex-basis: 100%;
    background-color: #fff;
    padding: 5px;
    /*flex-wrap: wrap;*/
    flex-direction: column;
    /*margin-bottom: 10px;*/
    min-height: 220px;
    /*background-color: lightpink;*/
}

/*左边区域下方部分*/
.main-left .left-box-bottom{
    flex-basis: 30%;
    background-color: #fff;
    padding: 5px;
    /*flex-wrap: wrap;*/
     flex-direction: column;
      min-height: 100px;
    /*background-color: lightblue;*/
}

/*增长曲线*/
/*.enterprise-msg .public-right .user-graph{
    background-color: skyblue;
}*/

/*左边区域上下两部分中的内容部分*/
.public-main{
    width: 100%;
    align-content: center;
    justify-content: space-between;
    margin-top: 10px;
    flex-direction: column;
    /*flex-wrap: wrap;*/
    /*border: 1px solid #000;*/
}
.public-main.report-main{
    flex-basis: 100%;
    padding-left: 10px;
}
.report-msg{
    flex-grow:1;
    border-bottom: 1px dashed #ccc;
    padding-top: 10px;
    height:50%;
}
.report-msg ul{
    flex-basis: 24%;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.report-msg ul li{
    margin-top: 5px;
    margin-bottom: 10px;
}
.report-msg .graph{
     flex-basis: 38%;
}
/*覆盖h+框架进度条颜色*/
.progress-bar {
    background-color: rgb(46, 141, 237);
}
