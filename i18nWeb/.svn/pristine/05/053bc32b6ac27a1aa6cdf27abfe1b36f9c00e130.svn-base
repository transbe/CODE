}
 @media (max-width:1000px) {
     /*头部样式变化*/
     .welcom-header {
         /*border: 1px solid red;*/
         flex-wrap: wrap;
         justify-content: space-between;
     }
     .welcom-header .header-model {
         flex-basis: 47%;
     }
     .welcom-header .header-model:first-child,
     .welcom-header .header-model:nth-child(2) {
         margin-bottom: 5px;
     }
     .welcom-header .header-model:last-child {
         margin-top: 5px;
     }





 }
 /*媒体查询，使其尽量可以适应手机*/
 
 @media (max-width: 800px) {
     .welcom-header .header-model {
         flex-basis: 100%;
         margin-bottom: 5px;
     }
     .welcom-header .header-model:last-child {
         margin-top: 0px;
     }
     .status-main {
         flex-wrap: wrap;
     }
     
 }