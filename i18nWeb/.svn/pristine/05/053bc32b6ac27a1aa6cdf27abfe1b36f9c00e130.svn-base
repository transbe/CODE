.header-title {
     flex-basis: 40%;
     white-space: nowrap;
     text-align: center;
     font-size: 16px;
 }
 
 .header-content {
     flex-basis: 60%;
     text-align: center;
     font-size: 16px;
     white-space: nowrap
 }




/*公共部分*/
.common-title {
     width: 100%;
     height: 40px;
     line-height: 40px;
     color: #333;
     border-bottom: 1px solid #eee;
 }
 
.title-left {
     flex-basis: 80%;
     text-align: left;
     /*border: 1px solid pink;*/
 }
 
.title-right {
     flex-basis: 20%;
     /*border: 1px solid blue;*/
     text-align: right
}
 
.title-style {
     font-size: 16px;
 }
 
.left-bg::before {
     display: inline-block;
     width: 5px;
     height: 25px;
     content: "";
     background: #f79324;
     margin-right: 5px;
     vertical-align: middle
 }
 
.right-arrows {
     font-size: 12px;
     color: #59b6fc;
 }
 
.right-arrows::after {
     display: inline-block;
     content: ">";
     color: #59b6fc;
     font-size: 12px;
     transform: scaleY(1.5);
     margin-left: 5px;
 }

 /*消息内容*/
.news-main {
     width: 100%;
 }

 /*消息列表样式*/
 .news-lists {
     width: 100%;
     font-size: 12px;
     overflow: hidden;
     /*background: yellow*/
 }
 
 .news-lists li {
     height: 80px;
     padding: 5px 0;
     border-bottom: 1px solid #eee;
     position: relative
 }
 
 .news-lists li a:hover {
     color: #fff
 }
 
 .news-lists li p {
     line-height: 16px;
 }
 
 .news-lists li p:first-of-type span:first-of-type {
     margin-right: 10px;
 }
 
 .news-lists li p:nth-of-type(2) {
     font-size: 14px;
     color: #444;
     /*font-weight: bold*/
 }
 
 .news-lists li p:nth-of-type(3) {
     width: 70%;
     /*border: 1px solid red;*/
     overflow: hidden;
     text-overflow: ellipsis;  /*设置超过隐藏  */
     overflow: hidden;
     white-space: nowrap;
     
 }
 
 .news-lists li:last-of-type {
     border-bottom: 0;
 }
 
 .news-lists .list-line1 {
     line-height: 20px;
 }
 
 .news-lists .news-check-btn {
     position: absolute;
     right: 0;
     bottom: 10px;
     display: block;
     width: 60px;
     height: 30px;
     line-height: 30px;
     text-align: center;
     color: #fff;
     font-size: 12px;
     border-radius: 4px;
     background: #2e8ded;
     z-index: 3
 }

@media (max-width:1300px) {
   .news-lists li p:nth-of-type(3){
         width: 70%;
         /*display: none;*/
         text-overflow: ellipsis;  /*设置超过隐藏  */
         overflow: hidden;
         white-space: nowrap;
   }
   .news-lists li p:nth-of-type(3):hover{
        text-overflow:inherit;
        overflow:visible; /*设置鼠标hover显示全部  */
   }
   .news-lists .news-check-btn {
    position: absolute;
    right: 0;
    bottom: 10px;
    display: block;
    width: 40px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    color: #fff;
    font-size: 12px;
    border-radius: 4px;
    background: #2e8ded;
    z-index: 3;  
}

