/*��������*/

.more-search {
    margin-top: 16px;
    display: none;
}

.table {
    border: 1px solid #dddddd
}

.tablebg {
    border: 1px solid #4aaffb;
    background: #edf7ff
}

td {
    white-space: nowrap;
    text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    overflow: hidden
}

.fixed-table-container {
    border: none
}

.bootstrap-table {
    margin-bottom: 20px
}

.fixed-table-container tbody tr:last-child {
    border-bottom: 1px solid #eee !important;
}


/*开始时间样式调整*/

.reg-time {
    flex-basis: 40%;
    flex-wrap: nowrap;
    justify-content: flex-start;
}

.reg-time input {
    flex-basis: 30%;
    border: 1px solid #ccc;
    border-radius: 4px;
    height: 34px;
    padding: 6px 12px;
}

.line {
    width: 20px;
    text-align: center;
    margin: 6px 12px;
}

.reg-time .reg-begin {
    margin-right: 7px;
    white-space: nowrap;
}

.form-horizontal .row-content:nth-child(2) .row-lab .checkbox label {
    vertical-align: middle;
    padding-left: 24px;
}

.body-height {
