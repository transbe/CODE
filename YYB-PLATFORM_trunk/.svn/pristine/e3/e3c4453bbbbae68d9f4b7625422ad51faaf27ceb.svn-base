.container-fluid {
    overflow: hidden;
    /*padding-left: 18px;*/
    margin-top: -5px;
}

.container-fluid .search_item_wrapper {
    padding-bottom: 15px;
    /*margin-bottom: 11px;*/
    background: #fff;
    border-bottom: 1px solid #cbccce;
    border-radius: 6px;
    box-shadow: 0px 1px 8px #e3e3e3;
    margin-left: -8px;
    margin-right: 15px;
}

.search_item_wrapper .top {
    padding-left: 12px;
}

.container-fluid .content {
    overflow: hidden;
    width: 100%;
}

.container-fluid .content .right_wrapper {
    margin-top: 0px
}

.container-fluid .content .search_wrapper {
    margin-right: -50px
}

.container-fluid .content #toolbar .export-all,
.container-fluid .content #toolbar .inviteuser {
    margin: 0 10px;
    color: #53b5fc;
    /*font-size: 14px;*/
    cursor: pointer
}

.container-fluid #table .th-inner {
    text-align: center
}

.search_btn_right {
    right: 80px;
    position: absolute;
    height: 36px;
    color: white;
    background: #58b7f7;
    width: 80px;
    text-align: center;
    line-height: 36px;
    border-radius: 18px;
    float: left;
    cursor: pointer
}


