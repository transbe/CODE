}
/*字体小标*/
.little{
    font-size: 12px;
    padding-top: 10px;
    margin-left: -5px;
}
/*红色字体*/
.red{
    color:#ff0000;
}
/*黑色字体*/
.bold{
    font-weight: 700;
}
/*蓝色字体*/
.blue{
    color:#5ab6fc;
}
/*拖拽的小手模式*/
.title{
    cursor: pointer;
    position: relative;
}
/*关闭按钮*/
.del{
    position: absolute;
    top:0px;
    right: 9px;
    font-size: 12px;
}