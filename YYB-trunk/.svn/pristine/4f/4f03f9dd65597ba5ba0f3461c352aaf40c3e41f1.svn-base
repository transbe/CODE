/*------*/
#page-wrapper {
    background: #edf1f2
}

#page-wrapper li,
#page-wrapper ul {
    list-style: none;
    margin: 0;
    /*padding: 6px 0;*/
    padding: 8px 11px;
}


/*------*/
a{
	 text-decoration: none !important;	 
}

.kuai2,
.kuai3,
.kuai4 {
    display: none
}

.feedback_img,
.feedback_img_file,
.left {
    float: left
}

#page-wrapper {
    background: #edf1f2
}

#page-wrapper .container-fluid {
    width: 100%;
    height: 100%
}

#page-wrapper .screening {
    font-size: 14px;
    background: #fff;
    border-radius: 5px;
    padding: 12px 10px;
    border: 1px solid #e1e5e6;
    box-shadow: 2px 1px #e1e5e6
}

#page-wrapper .screening .suspensionFrame {
    width: 275px;
    height: 235px;
    position: fixed;
    right: 35px;
    top: 200px;
    word-wrap: break-word;
    z-index: 98
}

#page-wrapper .screening .suspensionFrame .title {
    font-size: 20px;
    background: #59B6FC;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    position: relative
}

#page-wrapper .screening .suspensionFrame .zixun {
    position: absolute;
    top: -15px;
    left: -30px;
    z-index: 99
}

#page-wrapper .screening .suspensionFrame .content {
    width: 100%;
    height: 161px;
    background: #fff;
    opacity: .8;
    border: 1px solid #ececec;
    box-shadow: 1px 2px #ececec
}

#page-wrapper .screening .suspensionFrame .content p {
    margin: 0;
    padding-left: 20px;
    color: #333;
    font-size: 16px;
    height: 45px;
    line-height: 45px
}

.kuai1,
.kuai2 {
    height: 100%
}

#page-wrapper .screening .suspensionFrame .content p span {
    padding-left: 8px
}

.mt10 {
    margin-top: 10px
}

.center {
    text-align: center;
    border-bottom: 1px solid #ececec;
    line-height: 50px;
    font-size: 20px;
    cursor: pointer
}

.changeOne h1,
.changeOne ul li {
    line-height: 22px;
    word-break: normal
}

.body,
html {
    background: #edf1f2
}

.kuai2 img {
    margin: 10px 0;
    width: 280px;
    height: 320px
}

.kuai3 {
    height: 100%
}

.changeOne {
    width: 100%;
    height: 100%;
    padding-bottom: 20px
}

.changeOne ul {
    margin-left: 30px;
    margin-top: 5px
}

.changeOne ul li {
    list-style: disc;
    font-size: 16px;
    color: #666
}

.changeOne h1 {
    font-weight: 400;
    font-size: 18px;
    color: #59b6fc
}

.changeOne p {
    line-height: 20px;
    margin: 0;
    font-size: 16px;
    color: #666;
    word-break: normal
}

.changeOne .bgimg {
    line-height: 24px;
    padding-left: 30px
}

.comcss {
    padding-bottom: 10px;
    border-bottom: 1px solid #e4e4e4;
    overflow: hidden;
}

b {
    margin-top: 10px;
    font-weight: 700;
    display: block
}

.feedback_main {
    width: 620px;
    margin: 0 auto;
    padding-top: 20px;
    padding-bottom: 30px
}

.feedback_area {
    height: 280px
}

.feedback_area textarea {
    width: 100%;
    height: 210px;
    padding: 20px;
    border: 1px solid #ddd;
    resize: none;
    font-size: 16px
}

.feedback_area span {
    font-size: 12px;
    color: red;
    font-family: "宋体";
    margin-top: 2px;
    display: none
}

.feedback_way {
    height: 90px
}

.feedback_way input {
    width: 100%;
    height: 58px;
    line-height: 58px;
    text-indent: 1em;
    border: 1px solid #ddd;
    font-size: 16px
}

.feedback_images,
.feedback_images img {
    width: 80px;
    height: 80px
}

.feedback_img {
    padding-top: 10px
}

.feedback_images {
    float: left;
    margin-right: 20px;
    position: relative
}

.feedback_images span {
    width: 28px;
    height: 28px;
    cursor: pointer;
    background: url(../../images/help/close.png) no-repeat;
    display: inline-block;
    position: absolute;
    top: 0;
    right: 0;
    margin-right: -12px;
    margin-top: -10px
}

.feedback_img_add {
    width: 80px;
    height: 80px
}

.feedback_img_add span {
    width: 78px;
    height: 78px;
    text-align: center;
    display: inline-block;
    font-size: 16px;
    color: #bbb;
    line-height: 120px;
    border: 1px solid #ddd;
    cursor: pointer;
    background: url(../../images/help/add.png) center 20px no-repeat
}

.feedback_img_file input {
    display: none
}

.feedback_btn {
    clear: both;
    text-align: center;
    height: 50px
}

.feedback_btn span {
    width: 280px;
    height: 48px;
    display: inline-block;
    line-height: 48px;
    border: 1px solid #3496df;
    color: #fff;
    font-size: 20px;
    border-radius: 5px;
    text-align: center;
    cursor: pointer;
    background: #59b6fc
}

.feedback_success {
    height: 590px;
    display: none
}

.success_img {
    padding-top: 100px;
    text-align: center
}

.success_text {
    padding-top: 30px
}

.success_text p {
    font-size: 20px;
    color: #999;
    line-height: 40px;
    text-align: center
}

.limit {
    clear: both;
    padding-top: 10px;
    font-size: 16px;
    color: #999
}

.jian {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 0;
    font-size: 0;
    line-height: 0;
    border-width: 7px;
    border-style: solid dashed dashed;
    border-color: #67BDF6 transparent transparent
}

.row {
    margin-left: -100px;
    margin-right: 0
}

.return {
    width: 100px;
    height: 40px;
    margin: 0 auto
}

.return .btn {
    color: #59B6FC;
    font-size: 20px
}

.return .btn:hover {
    color: #59B6FC;
    text-decoration: none
}

.to_top {
    width: 40px;
    height: 40px;
    position: fixed;
    bottom: 20px;
    right: 20px;
    display: none;
}
.comcss img{
    width: 100%;
    vertical-align: middle;
    
}
.commcss-content{
    width: 100%;
    overflow: hidden;
}
