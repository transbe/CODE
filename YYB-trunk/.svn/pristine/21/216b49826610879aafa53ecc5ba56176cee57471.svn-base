.formula{
    border-style: dashed; 
    border-width: 1px 0px 0px 0px; 
}
.column-style{
    border-style: solid; 
    border-width: 0px 1px; 
    /*height:550px*/
}
.annotation-item{
    padding:0px 0px 0px 10%;
    margin: 0 0 2px;
}
label{
    font-weight:normal
}
.not-empty-mark {
    display: inline-block;
    height: 15px;
    line-height: 15px;
    padding-top: 3px;
    margin-right: 5px;
    color: red;
    vertical-align: middle;
}
.arithmetic_progression{
    display: none;
    margin-top: 20px;
}
@media (min-width: 1366px) { 
    .annotation-item{
        padding:0px 0px 0px 10%;
        margin: 0 0 10px;
    }
    .img-1{
        height:75px
    }
    .img-2{
        height:370px
    }
}
@media (max-width: 1365px) { 
    .annotation-item{
        padding:0px 0px 0px 10%;
        margin: 0 0 2px;
    }
    .img-1{
        height:50px
    }
    .img-2{
        height:180px
    }
}

#radius{
    border-bottom:1px dashed;
}

caption {
    padding-top: 2px;
    padding-bottom: 2px;
    color: rgb(119, 119, 119);
    text-align: center;
}
.table {
    margin-bottom: 8px;
}
.a-nowarp-hidden{
    white-space:nowrap;  
    text-overflow:ellipsis;
    overflow:hidden;
} 
