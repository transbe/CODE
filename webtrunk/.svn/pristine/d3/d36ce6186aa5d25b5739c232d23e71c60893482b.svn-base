.bootstro-finish-btn-wrapper {
    width: 80%;
    height: 30px;
    border-top: 1px solid #eee;
    padding-top: 5px;
    text-align: center;
    clear: both;
    margin-bottom: 0px;
    position: absolute;
    bottom: 10px;
}

.bootstro-nav-wrapper {
    margin-top: 10px;
}

.bootstro-next-btn,
.bootstro-prev-btn {
    float: right;
    margin-right: 5px;
    background-color: #92cef4 !important;
    border-color: #92cef4 !important;
}

.bootstro-finish-btn {
    float: right;
    margin-left: 15px;
    background-color: #f5d898 !important;
    border-color: #f5d898 !important;
}

.bootstro-nav-wrapper button,
.bootstro-finish-btn-wrapper button {
    font-size: 12px !important;
    padding: 3px 5px 3px 5px !important;
    font-weight: bold;
    color: #fff !important;
}

.bootstro-finish-btn-wrapper button:hover,
.bootstro-finish-btn-wrapper button:active {
    color: #fff !important;
}

.bootstro-finish-btn-wrapper .bootstro-finish-btn:hover,
.bootstro-finish-btn-wrapper .bootstro-finish-btn:active {
    background: #f5d898 !important;
}

.bootstro-finish-btn-wrapper .bootstro-prev-btn:hover,
.bootstro-finish-btn-wrapper .bootstro-prev-btn:active {
    background: #92cef4 !important;
}


/*修改*/

.label.label-success {
    background: #f7f7f7 !important;
    color: inherit !important;
    padding: 0px !important;
}

.hint-pic {
    width: 16px;
    height: 20px;
    margin-right: 5px;
    display: inline-block;
    vertical-align: middle;
    background: url("hint.png") no-repeat center;
}