.popover {
    min-width: 276px !important;
    max-width: 900px !important;
    z-index: 100064;
}

.popover .popover-content {
    position: relative;
    min-height: 120px;
    line-height: 25px;
    color: #c23903;
    font-size: 16px;
    padding-bottom: 40px;
}

.bootstro-backdrop {
    opacity: 0.7;
    background-color: #000000;
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index:1;
    filter: alpha(opacity=70);
}

.bootstro.bootstro-highlight {
    position: relative;
    border: 1px solid white !important;
    background-color: white !important;
    z-index: 99999;
}


