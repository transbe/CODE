li,
ul {
    list-style: none;
    margin: 0;
    /*padding: 6px 0;*/
    padding: 8px 11px;
}

a {
    text-decoration: none !important;
}

body {
    background: #edf1f2
}

.container-fluid {
    width: 100%;
    height: 100%;
    padding: 8px;
}

.screening {
    font-size: 14px;
    background: #fff;
    border-radius: 5px;
    border: 1px solid #e1e5e6;
    box-shadow: 2px 1px #e1e5e6;
    min-width: 750px;
}

.tab-header {
    height: 50px;
}

.kuai1,
.kuai2,
.kuai3,
.kuai4 {
    height: 100%
}

.kuai2,
.kuai3,
.kuai4 {
    display: none
}

.feedback_img,
.feedback_img_file,
.left {
    float: left
}

.screening .suspensionFrame {
    width: 270px;
    height: 230px;
    position: fixed;
    right: 15px;
    top: 200px;
    word-wrap: break-word;
    z-index: 98
}

.screening .suspensionFrame .title {
    font-size: 20px;
    background: #59B6FC;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    position: relative
}

.screening .suspensionFrame .zixun {
    position: absolute;
    top: -15px;
    left: -30px;
    z-index: 99
}

.screening .suspensionFrame .content {
    width: 100%;
    height: 161px;
    background: #fff;
    opacity: .8;
    border: 1px solid #ececec;
    box-shadow: 1px 2px #ececec
}

.screening .suspensionFrame .content p {
    margin: 0;
    padding-left: 20px;
    color: #333;
    font-size: 15px;
    height: 45px;
    line-height: 45px
}


/*.kuai1 {
    background: pink;
}*/

.screening .suspensionFrame .content p span {
    padding-left: 8px
}

.mt10 {
    margin-top: 10px
}

.center {
    text-align: center;
    border-bottom: 1px solid #ececec;
    line-height: 50px;
    font-size: 20px;
    cursor: pointer
}

.kuai2 img {
    margin: 10px 0;
    width: 280px;
    height: 320px
}

.changeOne {
    width: 100%;
    height: 100%;
    padding-bottom: 20px
}

.changeOne h1,
.changeOne ul li {
    line-height: 22px;
    word-break: normal
}

.changeOne ul {
    margin-left: 30px;
    margin-top: 5px
}

.changeOne ul li {
    list-style: disc;
    font-size: 16px;
    color: #666
}

.changeOne h1 {
    font-weight: 400;
    font-size: 18px;
    color: #59b6fc
}

.changeOne p {
    line-height: 20px;
    margin: 0;
    font-size: 16px;
    color: #666;
    word-break: normal
}

.comcss {
    padding-bottom: 10px;
    border-bottom: 1px solid #e4e4e4;
    overflow: hidden;
}

.comcss img {
    width: 100%;
    vertical-align: middle;
}

b {
    margin-top: 10px;
    font-weight: 700;
    display: block
}

.limit {
    clear: both;
    padding-top: 10px;
    font-size: 16px;
    color: #999
}

.jian {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 0;
    font-size: 0;
    line-height: 0;
    border-width: 7px;
    border-style: solid dashed dashed;
    border-color: #67BDF6 transparent transparent
}

.row {
    /*margin-left: -100px;*/
    margin-right: 0
}

.return {
    width: 100px;
    height: 40px;
    margin: 0 auto
}

.return .btn {
    color: #59B6FC;
    font-size: 20px
}

.return .btn:hover {
    color: #59B6FC;
    text-decoration: none
}

.to_top {
    width: 40px;
    height: 40px;
    position: fixed;
    bottom: 20px;
    right: 20px;
    display: none;
}


/*.commcss-content {
    width: 100%;
    overflow: hidden;
}*/


/*左侧盒子*/

.usual {
    float: left;
    width: 20%;
}


/*.usual img{
    width: 200px;
    height: 200px;
    margin-right: 12px;
    vertical-align: bottom;
}*/


/*右侧文字*/

.txt {
    float: left;
    width: 80%;
    background-color: skyblue;
}


/*小圆点*/

.txt li img {
    width: 1%;
    margin-right: 6px
}


/*字体小标*/

.little {
    font-size: 12px;
    padding-top: 10px;
    margin-left: -5px;
}


/*红色字体*/

.red {
    color: #ff0000;
}


/*黑色字体*/

.bold {
    font-weight: 700;
}


/*蓝色字体*/

.blue {
    color: #5ab6fc;
}


/*拖拽的小手模式*/

.title {
    cursor: pointer;
    position: relative;
}


/*关闭按钮*/

.del {
    position: absolute;
    top: 0px;
    right: 9px;
    font-size: 12px;
}


/*意见反馈部分样式*/

.feedback_main {
    width: 620px;
    margin: 0 auto;
    padding-top: 20px;
    padding-bottom: 30px
}

.feedback_area {
    height: 280px
}

.feedback_area textarea {
    width: 100%;
    height: 210px;
    padding: 20px;
    border: 1px solid #ddd;
    resize: none;
    font-size: 16px
}

.feedback_area span {
    font-size: 12px;
    color: red;
    font-family: "宋体";
    margin-top: 2px;
    display: none
}

.feedback_way {
    height: 90px
}

.feedback_way input {
    width: 100%;
    height: 58px;
    line-height: 58px;
    text-indent: 1em;
    border: 1px solid #ddd;
    font-size: 16px
}

.feedback_images,
.feedback_images img {
    width: 80px;
    height: 80px
}

.feedback_img {
    padding-top: 10px
}

.feedback_images {
    float: left;
    margin-right: 20px;
    position: relative
}

.feedback_images span {
    width: 28px;
    height: 28px;
    cursor: pointer;
    background: url(../../images/help/close.png) no-repeat;
    display: inline-block;
    position: absolute;
    top: 0;
    right: 0;
    margin-right: -12px;
    margin-top: -10px
}

.feedback_img_add {
    width: 80px;
    height: 80px
}

.feedback_img_add span {
    width: 78px;
    height: 78px;
    text-align: center;
    display: inline-block;
    font-size: 16px;
    color: #bbb;
    line-height: 120px;
    border: 1px solid #ddd;
    cursor: pointer;
    background: url(../../images/help/add.png) center 20px no-repeat
}

.feedback_img_file input {
    display: none
}

.feedback_btn {
    clear: both;
    text-align: center;
    height: 50px
}

.feedback_btn span {
    width: 280px;
    height: 48px;
    display: inline-block;
    line-height: 48px;
    border: 1px solid #3496df;
    color: #fff;
    font-size: 20px;
    border-radius: 5px;
    text-align: center;
    cursor: pointer;
    background: #59b6fc
}

.feedback_success {
    height: 590px;
    display: none
}

.success_img {
    padding-top: 100px;
    text-align: center
}

.success_text {
    padding-top: 30px
}

.success_text p {
    font-size: 20px;
    color: #999;
    line-height: 40px;
    text-align: center
}

.kuai3 .comcss ul,
.kuai3 .comcss li {
    list-style: none;
}


/*自定义常规简单layer弹框的样式*/

body .self,
body .self-alert {
    width: 300px;
    height: 180px;
    /*background: yellow;*/
    border-radius: 4px;
}

body .self .layui-layer-title,
body .self-alert .layui-layer-title {
    height: 40px !important;
    line-height: 30px;
    padding: 5px 0px 10px 20px;
    background: rgba(53, 64, 82, 0.9);
    text-align: left;
    color: #fff;
    font-size: 14px;
    border-radius: 4px 4px 0 0;
}

body .self .layui-layer-setwin,
body .self-alert .layui-layer-setwin {
    width: 18px;
    height: 18px;
    overflow: hidden;
}

body .self .layui-layer-ico.layui-layer-close1,
body .self-alert .layui-layer-ico.layui-layer-close1 {
    /*background-position: 20px 20px;*/
    /*background-position: 0 -40px;*/
    /*position: relative;
    border-right: 0px solid #fff;
    -webkit-filter: drop-shadow(0px 0 2px #fff);
    filter: drop-shadow( 0px 0 2px #fff);*/
}

body .self .layui-layer-ico,
body .self-alert .layui-layer-ico,
{
    /*background: url("../../images/common/icon.png") no-repeat;*/
}

body .self .layui-layer-ico::after,
body .self-alert .layui-layer-ico::after {
    content: "X";
    display: inline-block;
    width: 20px;
    height: 20px;
    line-height: 15px;
    font-size: 12px;
    color: #fff;
}

body .self .layui-layer-content,
body .self-alert .layui-layer-content {
    height: 90px;
    font-size: 12px;
    color: #666;
}

body .self .layui-layer-btn,
body .self-alert .layui-layer-btn {
    border-top: 1px solid #E9E7E7;
    text-align: center;
    padding: 5px 10px
}

body .self .layui-layer-btn .layui-layer-btn1,
body .self-alert .layui-layer-btn .layui-layer-btn1 {
    background: #f4b836;
    color: #fff;
    border-color: #f4b836;
}

body .self .layui-layer-content input {
    margin-top: 8px;
    line-height: 30px;
    width: 100%;
}


/*自定义 layer msg弹出框 */

body .self-msg {
    min-width: 80px;
    width: 180px;
    color: #fff;
    background: none;
    border: none;
    border-radius: 5px;
}

body .self-msg .layui-layer-content {
    text-align: center;
    /*background: linear-gradient(to bottom, #d3e9fa, #59b6fc);*/
    background: rgba(0, 0, 0, .5);
    border-radius: 5px;
}