* {
    margin: 0px;
    padding: 0px
}

#page-wrapper {
    background: #edf1f2;
    height: 100%
}

#page-wrapper .allenterprised input {
    display: block
}

#page-wrapper .allenterprised .radio {
    background: url("../images/login_img/radio.png") no-repeat;
    margin: 10px
}

#page-wrapper .allenterprised .on_check {
    background: url("../images/login_img/radio_checked.png") no-repeat
}

#page-wrapper .allenterprised .radiobox {
    opacity: 0;
    cursor: pointer;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
    width: 100%;
    height: 100%;
    display: inline-block
}

#page-wrapper .allenterprised span {
    margin-left: 5px
}

#page-wrapper .Loginset {
    background: #fff;
    border-radius: 5px;
    border: 1px solid #e1e5e6;
    padding: 12px 10px;
    box-shadow: 2px 1px #e1e5e6;
    font-size: 14px;
    height: 780px;
    margin: 10px 0px
}

#page-wrapper .Loginset .panel-heading {
    text-align: center
}

#page-wrapper .quite {
    text-align: center;
    margin: 20px 0px
}

#page-wrapper .quite button {
    background: #59b6fc;
    width: 120px;
    color: #fff
}


/*修改密码*/

.textAlign {
    text-align: center
}

.textRight {
    text-align: right
}

.modpwd .row {
    line-height: 30px;
    padding: 10px 0;
}

.modpwd input[type="password"] {
    border: 1px solid #ddd;
    border-radius: 4px;
    text-indent: 5px;
}