* {
    margin: 0px;
    padding: 0px
}

.mt60 {
    margin-top: 60px
}

.mt30 {
    margin-top: 30px
}

.certification {
    background: #fcfcfc;
    margin: 5px
}

.certification .certification_main .top {
    height: 54px;
    line-height: 54px
}

.certification .certification_main .top .top-border {
    border-bottom: 1px solid #d6d6d6
}

.certification .certification_main .top span {
    font-size: 16px;
    color: #333;
    font-weight: bold
}

.certification .certification_main .top .status {
    font-size: 16px;
    background-color: #ccc;
    color: #fff;
    padding: 5px;
    margin-left: 10px;
    border-radius: 4px
}

.certification .certification_main .content {
    margin-top: 20px
}

.certification .certification_main .content .des {
    margin-top: 20px;
    padding-bottom: 20px
}

.certification .certification_main .content .des p {
    font-size: 14px;
    color: #333;
    font-weight: bold
}

.certification .certification_main .content .des span {
    color: #666
}

.certification .certification_main .content .des .description {
    display: block;
    margin-top: 20px
}

.certification .certification_main .content .des .bottom {
    border-bottom: 1px solid #d6d6d6
}

.certification .certification_main .content .dismissed-reason {
    margin-top: 20px;
    font-size: 14px
}

.certification .certification_main .content .dismissed-reason p {
    color: #333;
    font-weight: bold
}

.certification .certification_main .content .dismissed-reason span {
    display: block;
    color: #f2252a;
    border: 1px solid #ececec;
    background-color: #fff;
    border-radius: 4px;
    padding: 10px;
    min-height: 80px
}

.certification .certification_main button {
    background: #59b6fc;
    color: #fff;
    height: 35px;
    line-height: 35px;
    border: 1px solid transparent;
    border-radius: 4px;
    padding: 0px 40px;
    margin: 60px 0px 80px 0px;
    font-size: 16px
}

.certification .enterpriseInformation {
    padding: 0;
    width: 780px;
    margin: 0 auto;
    background: #fcfcfc;
    padding-top: 20px
}

.certification .enterpriseInformation .top {
    width: 100%;
    height: 250px;
    border: 1px solid #e4e4e4;
    background: #fff
}

.certification .enterpriseInformation .top .title {
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    background: #F7F7F7;
    border-bottom: 1px solid #e4e4e4
}

.certification .enterpriseInformation .top .container {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 210px
}

.certification .enterpriseInformation .top .container .row {
    height: 30px;
    margin-left: 0px;
    margin-right: 0px
}

.certification .enterpriseInformation .top .container .row .left {
    text-align: right;
    padding-right: 0;
    height: 30px;
    line-height: 30px;
    color: #666
}

.certification .enterpriseInformation .top .container .row .right {
    height: 30px;
    padding-right: 0;
    line-height: 30px
}

.certification .enterpriseInformation .top .container .row .right input {
    width: 300px;
    height: 35px;
    border: 1px solid #e4e4e4;
    padding-left: 10px
}

.certification .enterpriseInformation .businessLicense {
    margin-top: 19px;
    margin-bottom: 10px;
    color: #666
}

.certification .enterpriseInformation .content {
    width: 100%;
    border: 1px solid #e4e4e4;
    padding: 20px 10px
}

.certification .enterpriseInformation .content .left {
    float: left
}

.certification .enterpriseInformation .content .business_img_add span,
.certification .enterpriseInformation .content .identify_img_add span,
.certification .enterpriseInformation .content span {
    border: none;
    width: 120px;
    height: 78px;
    text-align: center;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    font-size: 16px;
    color: #bbb;
    line-height: 120px;
    border: 1px solid #dddddd;
    cursor: pointer;
    background: url("/src/images/help/add.png") no-repeat center 20px
}

.certification .enterpriseInformation .content .identify_img_file input,
.certification .enterpriseInformation .content .business_img_file input {
    display: none
}

.certification .enterpriseInformation .content .identify_img_file,
.certification .enterpriseInformation .content .business_img_file {
    float: left
}

.certification .enterpriseInformation .content .business_enterprise_images,
.certification .enterpriseInformation .content .identify_enterprise_images {
    width: 120px;
    height: 80px;
    float: left;
    margin-right: 20px;
    position: relative
}

.certification .enterpriseInformation .content .business_enterprise_images img,
.certification .enterpriseInformation .content .identify_enterprise_images img {
    width: 120px;
    height: 80px
}

.certification .enterpriseInformation .content .business_enterprise_images span,
.certification .enterpriseInformation .content .identify_enterprise_images span {
    width: 28px;
    height: 28px;
    cursor: pointer;
    background: url("/src/images/enterprise/close.png") no-repeat;
    display: inline-block;
    *display: inline;
    *zoom: 1;
    position: absolute;
    top: 0px;
    right: 0px;
    margin-right: -12px;
    margin-top: -10px
}

.certification .enterpriseInformation .prompt {
    margin-top: 5px;
    color: #999;
    font-size: 14px;
    padding-left: 20px
}

.certification .enterpriseInformation button {
    background: #59b6fc;
    color: #fff;
    width: 120px;
    height: 35px;
    line-height: 35px;
    border: 1px solid transparent;
    border-radius: 4px;
    margin: 25px 0px;
    font-size: 16px
}

.certification .enterpriseInformation .note {
    color: #f22921;
    font-size: 12px;
    margin-left: 10px
}

.certification .enterpriseInformation .businessnote {
    color: #f22921;
    font-size: 12px;
    padding-left: 20px;
    padding-top: 10px
}

.enterprisetop {
    height: 54px;
    line-height: 54px
}

.enterprisetop .top-border {
    border-bottom: 1px solid #d6d6d6
}

.enterprisetop span {
    font-size: 16px;
    color: #333;
    font-weight: bold
}

.enterprisetop .status {
    font-size: 16px;
    background-color: #ccc;
    color: #fff;
    padding: 5px;
    margin-left: 10px;
    border-radius: 4px
}

.viewenterpriseInformation .content {
    height: 120px
}

.viewenterpriseInformation .identifyImg {
    margin-bottom: 10px
}

.viewenterpriseInformation .identify_list img {
    display: inline-block;
    margin: 0px 10px
}

.viewenterpriseInformation .business_list img {
    display: inline-block;
    margin: 0px 10px
}

.viewenterpriseInformation li {
    list-style: none;
    float: left
}