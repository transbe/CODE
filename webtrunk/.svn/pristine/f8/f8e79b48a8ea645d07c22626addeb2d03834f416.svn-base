/**  
 * @Last Modified by: lujingrui 
 * @Last Modified time: 2017 - 4 - 13
 * @Func: 测试桩管理主页面左侧树样式
*/

.ztree * {padding:0; margin:0; font-size:14px;}
.ztree {margin:0; padding:5px; color:#333}
.ztree li{padding:0; margin:0; list-style:none; line-height:24px; text-align:left; white-space:nowrap; outline:0}
.ztree li ul{ margin:0; padding:0 0 0 18px}
/*.ztree li ul.line{ background:url(./img/line_conn.png) 0 0 repeat-y;}*/

.ztree li a {padding-right:3px; margin:0; cursor:pointer; height:24px; color:#333; background-color: transparent; text-decoration:none; vertical-align:top; display: inline-block}
.ztree li a:hover {background-color:#2e8ded; height:24px;color:white;}
.ztree li a.curSelectedNode {padding-top:0px; background-color:#2e8ded; color:white; height:24px; opacity:1;}
.ztree li a.curSelectedNode_Edit {padding-top:0px; background-color:#2e8ded; color:black; height:24px; border:1px #666 solid; opacity:1;}
.ztree li a.tmpTargetNode_inner {padding-top:0px; background-color:#aaa; color:white; height:24px; border:1px #666 solid;
  opacity:0.8; filter:alpha(opacity=80)}
.ztree li a.tmpTargetNode_prev {}
.ztree li a.tmpTargetNode_next {}
.ztree li a input.rename {height:24px; width:80px; padding:0; margin:0;
  font-size:14px; border:1px #585956 solid; *border:0px}
.ztree li span {line-height:24px; margin-right:2px; margin-left:6px; }
.ztree li span.button {line-height:0; margin:0; padding: 0; width:21px; height:24px; display: inline-block; vertical-align:middle;
  border:0 none; cursor: pointer;outline:none;
  background-color:transparent; background-repeat:no-repeat; background-attachment: scroll;
  background-image:url("./img/new.png"); }



.ztree li span.button.switch {width:21px; height:24px}
.ztree li span.button.root_open{background-position:-190px -8px;}
.ztree li span.button.root_close{background-position:-171px -8px}
.ztree li span.button.roots_open{background-position: -190px -8px;}
.ztree li span.button.roots_close{background-position: -171px -8px;}
.ztree li span.button.center_open{background-position: -190px -8px;}
.ztree li span.button.center_close{background-position: -171px -8px;}
.ztree li span.button.bottom_open{background-position: -190px -8px;}
.ztree li span.button.bottom_close{background-position: -171px -8px;}
.ztree li span.button.noline_open{background-position: -190px -8px;}
.ztree li span.button.noline_close{background-position: -171px -8px;}
.ztree li span.button.root_docu{ background:none;}
.ztree li span.button.roots_docu{background:none;}
/*.ztree li span.button.roots_docu{background-position: -190px -8px;}*/
.ztree li a.curSelectedNode span.button.roots_docu{background-position: -78px -7px;}
.ztree li a:hover span.button.roots_docu{background-position: -84px -6px;}
/*.ztree li span.button.center_docu{background-position: -190px -8px;}*/
.ztree li span.button.center_docu{background:none;}
/*.ztree li span.button.bottom_docu{background-position: -190px -8px;}*/
/*.ztree li span.button.bottom_docu{background-position: -190px -8px;}*/
.ztree li span.button.bottom_docu{background:none;}
.ztree li span.button.noline_docu{ background:none;}

.ztree li span.button.ico_open{margin-right:2px; background-position: -8px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a.curSelectedNode span.button.ico_open{margin-right:2px; background-position: -33px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a:hover span.button.ico_open{margin-right:2px; background-position: -33px -7px; vertical-align:top; *vertical-align:middle}

.ztree li span.button.ico_close{margin-right:2px; margin-right:2px; background-position:  -8px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a.curSelectedNode span.button.ico_close{margin-right:2px; margin-right:2px; background-position:  -33px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a:hover span.button.ico_close{margin-right:2px; margin-right:2px; background-position:  -33px -7px; vertical-align:top; *vertical-align:middle}

.ztree li span.button.ico_docu{margin-right:2px; background-position:  -60px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a.curSelectedNode span.button.ico_docu{margin-right:2px; background-position:  -86px -7px; vertical-align:top; *vertical-align:middle}
.ztree li a:hover span.button.ico_docu{margin-right:2px; background-position:  -86px -7px; vertical-align:top; *vertical-align:middle}

.ztree li span.button.edit {margin-left:2px; margin-right: -1px; background-position: -128px -7px; vertical-align:top; *vertical-align:middle}
.ztree li span.button.edit:hover {
  background-position: -128px -7px;
}
.ztree li span.button.remove {margin-left:2px; margin-right: -1px; background-position: -148px -7px; vertical-align:top; *vertical-align:middle}
.ztree li span.button.remove:hover {
  background-position: -148px -7px;
}
.ztree li span.button.add {margin-left:20px; margin-right: -1px; background-position: -107px -7px; vertical-align:top; *vertical-align:middle}
.ztree li span.button.add:hover {
  background-position: -107px -7px;
}
.ztree li span.button.ico_loading{margin-right:2px; background:url(./img/loading.gif) no-repeat scroll 0 0 transparent; vertical-align:top; *vertical-align:middle}

ul.tmpTargetzTree {background-color:#FFE6B0; opacity:0.8; filter:alpha(opacity=80)}

span.tmpzTreeMove_arrow {width:16px; height:24px; display: inline-block; padding:0; margin:2px 0 0 1px; border:0 none; position:absolute;
    background-color:transparent; background-repeat:no-repeat; background-attachment: scroll;
    background-position:-171px -84px; background-image:url("./img/new.png"); }

ul.ztree.zTreeDragUL {margin:0; padding:0; position:absolute; width:auto; height:auto;overflow:hidden; background-color:#cfcfcf; border:1px #00B83F dotted; opacity:0.8; filter:alpha(opacity=80)}
.ztreeMask {z-index:10000; background-color:#cfcfcf; opacity:0.0; filter:alpha(opacity=0); position:absolute}
