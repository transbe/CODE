body,
html {
    background-color: #edf1f2
}

.page_bg_wrapper {
    background: #edf1f2;
    padding: 15px
}

.search_item_wrapper {
    padding-bottom: 15px;
    /*margin-bottom: 15px;*/
    background: #fff;
    border-bottom: 1px solid #cbccce;
    border-radius: 6px;
    box-shadow: 0 1px 8px #e3e3e3
}

.search_item_wrapper .search_title {
    line-height: 34px
}

.search_item_wrapper .search_title .btn_wrapper {
    margin-top: 15px;
    padding: 0;
    text-align: center;
    color: #999
}

.search_item_wrapper .search_title .btn_wrapper .item {
    border: 1px solid #e3e3e3;
    cursor: pointer;
    padding: 0
}

.search_item_wrapper .search_title .btn_wrapper .item.active {
    border-color: #58b7f7;
    background: #edf7ff;
    color: #58b7f7
}

.search_item_wrapper .search_title .btn_wrapper .item:hover {
    background: #edf7ff
}

.search_item_wrapper .search_title .btn_wrapper .item:first-child {
    border-right: none;
    border-radius: 16px 0 0 16px
}

.search_item_wrapper .search_title .btn_wrapper .item:last-child {
    border-left: none;
    border-radius: 0 16px 16px 0
}

.search_item_wrapper .search_title .right_wrapper {
    margin-top: 15px
}

.search_item_wrapper .search_title .right_wrapper .search_wrapper {
    margin-right: 250px;
    position: relative
}

.search_item_wrapper .search_title .right_wrapper .search_wrapper .input_wrapper {
    border-radius: 16px 0 0 16px;
    height: 36px;
    margin-right: 80px;
    border: 1px solid #e3e3e3;
    padding-left: 30px;
    background: url(/src/images/task/icon_search.png) 10px center no-repeat
}

.search_item_wrapper .search_title .right_wrapper .search_wrapper .input_wrapper input {
    color: #333;
    float: left;
    height: 34px;
    width: 100%;
    outline: 0;
    -webkit-appearance: none;
    border: none
}

.search_item_wrapper .search_title .right_wrapper .search_wrapper .mybtn {
    height: 36px;
    color: #fff;
    background: #58b7f7;
    border-radius: 0 16px 16px 0;
    width: 80px;
    float: right;
    text-align: center;
    position: absolute;
    right: 0;
    top: 0;
    cursor: pointer
}

.search_item_wrapper .search_title .right_wrapper .search_btn_right {
    margin-right: 15px;
    position: absolute;
    right: 0;
    top: 0;
    width: 220px
}

.search_item_wrapper .search_title .right_wrapper .search_btn_right .more {
    border: 1px solid #58b7f7;
    color: #58b7f7;
    cursor: pointer;
    border-radius: 16px;
    text-align: center;
    width: 120px;
    padding-right: 10px;
    line-height: 34px;
    float: right;
    background: url(/src/images/task/icon_down1.png) 90px center no-repeat #edf7ff
}

.search_item_wrapper .search_title .right_wrapper .search_btn_right .more.active {
    color: #fff;
    background: url(/src/images/task/icon_up1.png) 90px center no-repeat #58b7f7
}

.search_item_wrapper .search_title .right_wrapper .search_btn_right .search_reset {
    height: 36px;
    color: #fff;
    background: #58b7f7;
    width: 80px;
    text-align: center;
    line-height: 36px;
    border-radius: 18px;
    float: left;
    cursor: pointer
}

.search_item_wrapper .search_title .right_wrapper .search_btn_right .search_reset:hover {
    background: #75c3fe
}

.search_item_wrapper .more_item_wrapper {
    padding-top: 15px;
    display: none
}

.search_item_wrapper .more_item_wrapper .sub_wrapper {
    position: relative;
    border: 1px solid #e3e3e3;
    padding: 15px;
    background: #f7f7f7
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .up_triangle {
    width: 0;
    height: 0;
    font-size: 0;
    border-width: 0 10px 10px;
    border-style: solid;
    border-color: transparent transparent #e3e2e2;
    position: absolute;
    right: 42px;
    top: -10px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .up_triangle i {
    display: block;
    width: 0;
    height: 0;
    border-width: 0 10px 10px;
    border-style: solid;
    border-color: transparent transparent #f7f7f7;
    position: absolute;
    top: 1px;
    left: -10px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper div {
    /*overflow: hidden*/
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line {
    line-height: 32px;
    position: relative;
    overflow: hidden;
    padding: 6px 0
}
.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line-height {
    line-height: 32px;
    position: relative;
    /*height: 40px;*/
    padding: 6px 0
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .title {
    position: absolute;
    width: 80px;
    text-align: right;
    color: #666
}


/*.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .title:after {
    content: '：'
}*/

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box {
    margin-left: 95px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box b {
    float: left;
    font-weight: 400;
    margin: 0 4px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item2 div {
    display: none
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item2 div.active {
    display: block
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item {
    float: left;
    position: relative;
    margin-right: 36px;
    cursor: pointer
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item:last-of-type {
    margin-right: 0
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item.active,
.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item:hover {
    color: #58b7f7
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .item i {
    padding-left: 8px;
    font-size: 12px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date .itemBtn,
.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .itemBtn {
    float: left;
    position: relative;
    margin-right: 36px;
    cursor: pointer;
    margin-left: 10px;
    width: 54px;
    text-align: center;
    color: #fff;
    background: #999;
    border-radius: 16px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date .itemBtn.active,
.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .itemBtn.active {
    background: #58b7f7
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date .itemBtn:hover,
.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .itemBtn:hover {
    background: #58b7f7;
    color: #fff
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date {
    float: left
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date .diy.active {
    color: #58b7f7
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date span {
    float: left
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .date input {
    line-height: 30px;
    float: left;
    height: 30px;
    border: 1px solid #e3e3e3;
    outline: 0;
    -webkit-appearance: none;
    color: #999;
    padding-left: 8px;
    width: 118px;
    cursor: pointer;
    background: url(/src/images/task/icon_down2.png) 98px center no-repeat #fff;
    background-size: 12px 6px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper {
    float: left
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .peo_border {
    background: #fff;
    float: left;
    border: 1px solid #e3e3e3;
    height: 32px;
    line-height: 30px;
    width: 320px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .peo_border .peo_selected {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    padding-left: 10px;
    float: left;
    width: 280px;
    height: 30px;
    outline: 0;
    -webkit-appearance: none;
    border: none;
    cursor: pointer
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .peo_border .mybtn {
    cursor: pointer;
    float: right;
    width: 32px;
    height: 30px;
    /*background: url(/src/images/common/icon_plus.png) center center no-repeat*/
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .clear {
    color: #999;
    cursor: pointer;
    margin-left: 10px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line .item_box .peo_wrapper .clear:hover {
    color: #58b7f7
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .btn_wrapper {
    position: relative;
    height: 20px
}

.search_item_wrapper .more_item_wrapper .sub_wrapper .btn_wrapper .clear {
    cursor: pointer;
    color: #58b7f7;
    float: right;
    padding: 0 22px;
    line-height: 18px;
    /*background: url(/src/images/common/icon_clear.png) 0 center no-repeat*/
}

.table_wrapper {
    border-bottom: 1px solid #cbccce;
    border-radius: 6px;
    box-shadow: 0 1px 8px #e3e3e3;
    background: #fff;
    text-align: center;
    padding-bottom: 15px
}

.line-height .form_datetime{
    display: inline-block !important;
    width: 220px !important;
}

.bootstrap-select.form-control:not([class*="col-"]) {
    width: 220px;
}

#recorder{
    width: 220px;
    display: inline;
}



.row.line-height>div:nth-of-type(1){
    padding-left: 30px
}
.row.line-height>div:nth-of-type(2){
    padding-left: 20px
}

/*媒体查询小于1800*/
@media screen and (max-width:1800px) {
    .line-height .form_datetime{
        display: inline-block !important;
        width: 300px !important;
    }

    .bootstrap-select.form-control:not([class*="col-"]) {
        width: 300px;
    }

    #recorder{
        width: 300px;
        display: inline;
    }
}
/*媒体查询小于1366*/
@media screen and (max-width:1366px) {
    .line-height .form_datetime{
        display: inline-block !important;
        width: 220px !important;
    }

    .bootstrap-select.form-control:not([class*="col-"]) {
        width: 220px;
    }

    #recorder{
        width: 220px;
        display: inline;
    }
}
/*媒体查询小于1200*/
@media screen and (max-width:1200px) {
    .row.line-height>div{
        padding-left: 30px !important;
    }
    .search_item_wrapper .more_item_wrapper .sub_wrapper .son_wrapper .line-height {
        height: 118px;
    }

    .line-height .form_datetime{
        width: 180px !important;
    }
     .bootstrap-select.form-control:not([class*="col-"]) {
        width: 180px;
    }

}
/*媒体查询小于1050px*/
@media screen and (max-width:1050px) {
    .line-height .form_datetime{
        width: 120px !important;
    }
     .bootstrap-select.form-control:not([class*="col-"]) {
        width: 120px;
    }
}


.itemBtn{
    /*float: left;*/
    position: relative;
    /*margin-right: 36px;*/
    cursor: pointer;
    margin-left: 10px;
    width: 54px;
    text-align: center;
    color: #fff;
    background: #999;
    border-radius: 16px;
    display: inline-block;
}

.itemBtn.active {
    background: #58b7f7;
}
.itemBtn:hover {
    background: #58b7f7;
}

#dateBtn{
    margin-left: 6px;
}