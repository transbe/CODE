            <div class="popover-content">
                <p id="content-title"></p>
                <img src="" id="content-img" style="width:480px;height:240px">
                <div class="bootstro-finish-btn-wrapper">
                    <button class="btn  btn-xs bootstro-finish-btn"><i class="icon-ok"></i>关闭</button>
                    <button class="btn  btn-xs bootstro-next-btn">下一步</button>
                    <button class="btn  btn-xs bootstro-prev-btn">上一步</button>
