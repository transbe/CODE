// 企业ID
var enterpriseId;

// token
var token;

// 主键
var objectId;

// 组织机构Id
var orgId;

// 初始化方法
$(function() {
	var userBo=JSON.parse(lsObj.getLocalStorage("userBo"));
	token=lsObj.getLocalStorage("token")
	enterpriseId=userBo.enterpriseId;
	orgId=userBo.orgId;
	// loadDeptSelect();
	objectId=getParameter("objectId");
	// alert(objectId);
	if(objectId!=null&&objectId!=""&&objectId!=undefined){
		var createUserName=decodeURI(getParameter("createUserName"));
		$.ajax({
		url:"/cloudlink-core-framework-1/user/getById?token="+token+"&objectId="+objectId+"&enterpriseId="+enterpriseId,
		contentType: "application/json; charset=utf-8",
		dataType:"json",
		method:"get",
		success:function(result){
			console.log("id"+JSON.stringify(result));
			if(result.success==1){
				var data=result.rows[0];
				var radio = $('.userType');
				for (var i = 0; i < radio.length; i++) {
					if (radio[i].value == data.userType) {
						radio[i].checked=true; 
					}
				}
				var status=$(".status");
				for (var i = 0; i < status.length; i++) {
					if (status[i].value == data.status) {
						status[i].checked=true; 
					}
				}
				$("#objectId").val(objectId);
				$("#account").val(data.account);
				$("#user_name").val(data.userName);
				// $("#pass_word").val(data.password);
				$("#createTime").val(data.createTime);
				$("#createUser").val(createUserName);
				$("#mobileNum").val(data.mobileNum);
				$("#password_hide").hide();
				$("#status_show").show();
				$("#createuser_show").show();
				$("#createtime_show").show();
			}else{
				// parent.layer.confirm("加载用户信息失败，请重试",{btn:0,time:2000,skin:'message-skin',closeBtn: 0});
				layer.confirm('加载用户信息失败，请重试!', {
					btn: ['确定'], //按钮
					skin: 'self'
				});
			}
		}
	 });
	}

	//表格验证
	$('#addCollectionForm').bootstrapValidator({
		message: 'This value is not valid',
		feedbackIcons: {
			　　　　　　　　valid: 'glyphicon glyphicon-ok',
			　　　　　　　　invalid: 'glyphicon glyphicon-remove',
			　　　　　　　　validating: 'glyphicon glyphicon-refresh'
		　　　　　　　　   },
		fields: {
			// account: {
			// 	trigger: 'keyup',
			// 	message: '账户验证失败',
			// 	validators: {
			// 		notEmpty: {
			// 			message: '账户不能为空'
			// 		}
			// 	}
			// },
			user_name: {
				trigger: 'keyup',
				message: '检测单位(人员)验证失败',
				validators: {
					notEmpty: {
						message: '检测单位(人员)不能为空'
					},
					regexp: {
                            regexp: /^[\u4e00-\u9fa5A-Za-z0-9]+$/,
                            message: '用户名只能包含大写、小写、数字和汉字'
                    }
				}
			},
			pass_word: {
				trigger: 'keyup',
				message: '密码验证失败',
				validators: {
					notEmpty: {
						message: '密码不能为空'
					},
					stringLength: {
                            min: 6,
                            max: 18,
                            message: '密码长度必须在6到18位之间'
                    }
				}
			},
			mobileNum: {
				trigger: 'keyup',
				validators: {
					notEmpty: {
						message: '电话号码不能为空'
					},
					regexp: {
                            regexp: /^1[34578]\d{9}$/,
                            message: '请输入正确格式的电话号码'
                    }
				}
			}
		}
	});
	// alert($("#user_name").val());
});
