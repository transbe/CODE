/*flex布局查询条件部分样式*/

.flex-box {
    display: flex;
    display: -webkit-flex;
}


/*#btnCheckNone1 {
margin-bottom: 15px;
justify-content: flex-start;
}*/

.piplemodel-content {
    flex-basis: 90%;
    justify-content: flex-start;
}

.piplemodel-lab:first-of-type {
    flex-basis: 17%;
    margin-right: 2%;
}

.piplemodel-lab:last-of-type {
    flex-basis: 80%;
}

.piplemodel-lab {
    align-items: center;
}

.piplemodel-lab label {
    white-space: nowrap;
    font-weight: normal;
    vertical-align: top;
    margin-bottom: 0px;
}


/*.piplemodel-lab:first-of-type .pipe-input * {
    width: 100%;
    border: 1px solid #bbb;
    border-radius: 4px;
    height: 30px;
}*/

.piplemodel-content .piplemodel-lab:first-of-type .pipe-input {
    width: 100%;
    border: 1px solid #ddd;
    border-radius: 4px;
    height: 30px;
}

.piplemodel-btn {
    flex-basis: 15%;
    align-items: flex-end;
    justify-content: flex-end
}

.piplemodel-btn-type:first-of-type {
    margin-right: 10px;
}

#clearBtnHr {
    background-color: #f4b836;
    border: 1px solid #f4b836;
}

.pipeinputs {
    display: inline-block;
    margin-left: 5px;
}

.pipeinputs * {
    vertical-align: top;
}

.pipeinputs input {
    margin-top: 2px;
}

@media screen and (max-width:1400px) {
    .piplemodel-content {
        flex-wrap: wrap;
    }
    .piplemodel-content .piplemodel-lab {
        flex-basis: 100%;
    }
    .piplemodel-content .piplemodel-lab:first-of-type {
        margin-bottom: 10px;
    }
    .piplemodel-content .piplemodel-lab:last-of-type .pipe-input {
        margin-top: -12px;
    }
    .piplemodel-lab label {
        flex-basis: 13%;
        text-align: right
    }
    .piplemodel-lab .pipe-input {
        flex-basis: 80%;
    }
    .piplemodel-lab:first-of-type .pipe-input {
        /*width: 30%;*/
        flex-basis: 30%;
        margin-left: 7px;
    }
    .piplemodel-lab:last-of-type .pipe-input .pipeinputs {
        margin-top: 10px;
    }
}

@media screen and (max-width:1200px) {
    /*.piplemodel-lab:last-of-type label {
    margin-top: -20px;
}*/
    .piplemodel-lab:last-of-type .pipe-input {
        flex-wrap: wrap;
    }
}

.add-root {
    position: absolute;
    right: 10px;
    top: 8px;
}

.add-root:hover {
    cursor: pointer;
}

.ishide i {
    transform: rotate(180deg);
}


/*拖放css*/

.divider:hover {
    cursor: e-resize;
}

.divider i:hover {
    cursor: pointer;
}


