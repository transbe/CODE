.fixed-table-body {
    background-color: #fff;
}

.break-number {
    word-wrap: break-word;
    word-break: break-all;
    white-space: normal;
}


/*ztree右键样式*/

.treeRightMenu {
    position: absolute;
    visibility: hidden;
    top: 0;
    background-color: #f5f5f5;
    border: 1px solid #979797;
    box-shadow: 2px 2px 2px #999;
    text-align: left;
    padding: 2px;
    margin-bottom: 0px !important;
}

.treeRightMenu li {
    list-style: none;
    /*margin: 1px 0 0 0px;
    cursor: pointer;
    line-height: 3em;
    padding: 0 2em;
    border:solid 1px #e2e3e3;
    border-width:0 0 1px 1px;
    text-decoration: none;
    text-shadow: 1px 1px 0 #fff;
    white-space: nowrap;
    box-shadow:0 1px 0 rgba(255,255,2555,1)*/
}

.treeRightMenu li>a {
    display: block;
    padding: 0 2em 0 2em;
    text-decoration: none;
    width: auto;
    color: black;
    white-space: nowrap;
    line-height: 2.4em;
    text-shadow: 1px 1px 0 white;
    border-radius: 1px;
}

.treeRightMenu li>a>i {
    text-decoration: none;
    display: inline-block;
    width: 2.4em;
    height: 2.4em;
    background: transparent;
    margin: 0 0 0 -2em;
    vertical-align: top;
    text-align: center;
    line-height: 2.4em;
}

.treeRightMenu li>a .vakata-contextmenu-sep {
    display: inline-block;
    width: 1px;
    height: 2.4em;
    background: white;
    margin: 0 0.5em 0 0;
    border-left: 1px solid #e2e3e3;
}

.treeRightMenu li:hover {
    background: #e8eff7;
    box-shadow: 0 0 2px #0a6aa1;
}


