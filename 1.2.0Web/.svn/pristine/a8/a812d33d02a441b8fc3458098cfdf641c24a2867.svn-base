/**
 * @desc 获取申请状态中文
 * @method convertApplyStatus
 * @param {*String} applyStatus "0"/"1"/"2"/"3"/"4"/"5"
 * @return {*String} applyStatus 中文
 */
function convertApplyStatus(applyStatus) {
	switch (applyStatus) {
		case '0':
			applyStatus = '提交申请';
			break;
		case '1':
			applyStatus = '完善数据';
			break;
		case '2':
			applyStatus = '关闭申请';
			break;
		case '3':
			applyStatus = '提交报告';
			break;
		case '4':
			applyStatus = '修订报告';
			break;
		case '5':
			applyStatus = '验收通过';
			break;
	}
	return applyStatus;
}