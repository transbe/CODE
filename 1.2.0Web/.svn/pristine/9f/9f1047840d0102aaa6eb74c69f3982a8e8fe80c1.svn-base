/*非空标记*/

.star-mark {
    position: relative;
    top: 1px;
    display: inline-block;
    /*-webkit-font-smoothing: antialiased;*/
}

.star-mark::before {
    position: absolute;
    display: inline-block;
    content: "*";
    font-size: 16px;
    font-weight: bold;
    color: red;
    margin-left: -10px;
    vertical-align: middle;
}

.not-empty-mark {
    display: inline-block;
    height: 15px;
    line-height: 15px;
    padding-top: 3px;
    margin-right: 5px;
    color: red;
    vertical-align: middle;
}


/*历史记录样式*/

.fieldset {
    margin: 0px 10px;
}

.fieldset fieldset {
    padding: .35em .625em .75em;
    margin: 0 2px;
    border: 1px solid silver
}

.fieldset legend {
    padding: .5em;
    border: 0;
    width: auto;
    font-size: 14px;
    margin-bottom: 0px;
}

.fieldset legend span {
    display: inline-block !important;
    width: 14px;
    height: 14px;
    line-height: 14px;
    text-align: center;
    border-radius: 2px;
    border: 1px solid silver;
    font-size: 10px;
    color: #666;
    margin-left: 3px;
}

.fieldset legend span:first-child {
    margin-left: 10px;
}

.fieldset legend span.glyphicon {
    font-weight: 900;
    font-family: '';
}

.fieldset legend *::before {
    position: relative;
    top: -1px;
}

.fieldset fieldset * {
    padding: 0;
    margin: 0;
}

.fieldset fieldset ul {
    margin-top: -10px;
    margin-bottom: 10px;
}

.fieldset fieldset ul li {
    /*height: 30px;*/
    /*line-height: 30px;*/
    padding-left: 5px;
    vertical-align: middle;
    list-style: none;
    list-style-position: outside;
    /*border: 1px solid yellow;*/
    position: relative;
}

.fieldset fieldset ul li div {
    height: 30px;
    line-height: 30px;
    vertical-align: bottom;
}

.fieldset fieldset ul li span {
    margin-right: 5px;
}

.fieldset .time_line_mark {
    display: inline-block;
    width: 15px;
    /*height: 30px;*/
    /*border: 1px solid red;*/
    position: absolute;
    /*top: 5px;*/
    margin-right: 5px;
}

.fieldset .time_line_mark span {
    display: inline-block;
    background: #59b6fc;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    margin-left: 0px;
}

.fieldset .time_line_mark .line {
    width: 2px;
    /*height: 28px;*/
    height: 99%;
    border-radius: 1px;
}

.fieldset .time_line_mark .circle {
    width: 8px;
    height: 8px;
    border-radius: 50%;
    top: 45%;
    left: 25%;
    transform: translateY(-50%);
}

.fieldset fieldset ul li:first-of-type .time_line_mark .line {
    height: 13px;
    top: 13px;
}

.fieldset fieldset ul li div:last-of-type {
    /*border: 1px solid blue;*/
    margin-left: 20px;
}

.fieldset fieldset ul li div:last-of-type span {
    display: inline-block;
}


/*清除ie后input里面的小差号*/

::-ms-clear,
::-ms-reveal {
    display: none;
}

/*设置下拉框高度*/
.select-style .btn {
    height: 100%;
}