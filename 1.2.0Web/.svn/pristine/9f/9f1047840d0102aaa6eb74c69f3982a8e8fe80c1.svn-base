body {
    background-color: #e7e8ec !important;
    font-size: 13px;
    text-underline-position: none;
}


/*
 * 每个页面模块的划分
 */

.content-box {
    padding: 5px;
    overflow: hidden;
}

.content-header {
    margin-bottom: 5px;
    background-color: #fff;
    border: 1px solid transparent;
    border-radius: 4px;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
    box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
    border-color: #ddd;
    padding: 5px;
}

.content-body {
    width: 100%;
    height: 100%;
    background: #fff;
    border-radius: 4px;
    padding: 5px 5px;
}


