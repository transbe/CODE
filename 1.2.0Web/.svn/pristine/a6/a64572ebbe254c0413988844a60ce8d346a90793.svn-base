ul,
li {
    padding: 0;
    margin: 0;
}


/*左侧菜单收缩按钮*/

.nav-control {
    position: absolute;
    top: 50%;
    right: -8px;
    height: 50px;
    border: 8px solid #344052;
    border-top-color: transparent;
    border-bottom-color: transparent;
    border-right-width: 0;
    border-left-width: 8px;
    cursor: pointer;
    text-align: center;
    line-height: 34px;
    color: #fff;
}

.nav-control i {
    margin-left: -8px;
    transform-origin: center;
    transition-delay: 0;
    transition-duration: .1s;
    transition-property: transform;
    transition-timing-function: linear;
}

.mini-navbar .nav-control i {
    transform: rotate(180deg);
}


/*首页头部样式*/

.navbar-header {
    width: 46%;
}

.navbar-header .logo-name {
    display: inline-block;
    width: 137px;
    height: 30px;
    margin: 15px;
    /*background: url("../images/skin/skin_chip/skin0/logo.png") no-repeat center right;*/
}

.navbar-header .logo-info {
    display: inline-block;
    width: 185px;
    height: 19px;
    margin-bottom: 15px;
    /*background: url("../images/skin/skin_chip/skin0/logo1.png") no-repeat center left;*/
}

.navbar-static-top {
    margin-bottom: 0;
}

.person-operate {
    height: 60px;
}

.user-info-model {
    position: relative;
}

.user-info {
    min-width: 110px;
    background: transparent !important;
    padding-left: 50px;
}

.person-operate .user-info-model .user-info img {
    width: 30px;
    height: 30px;
    border-radius: 50%;
}

.person-operate.navbar-top-links>li>a {
    padding: 18px 10px !important
}

.person-operate.nav .open>a {
    padding: 17px 10px 17px 30px!important
}

.currentEnterpriseLi span {
    margin-right: 10px;
    line-height: 20px;
    display: inline-block
}


/*头部个人信息样式*/

.person-name {
    font-weight: 300;
    font-size: 12px;
}

.person-operate li {
    position: relative
}

.person-operate li a:hover {
    background: #434e62 !important;
}

.person-operate li a img {
    width: 20px;
    height: 20px;
    vertical-align: middle
}


/*.person-operate li:first-of-type a img {
    width: 30px;
    height: 30px;
}*/

.person-operate li a span {
    color: #fff;
    margin-left: 5px;
}

.person-operate li:nth-of-type(2) {
    position: relative
}


