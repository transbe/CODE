/*个人设置*/

.hide-person-operate {
    position: absolute;
    left: 0;
    top: 60px;
    width: 100%;
    min-width: 110px;
    text-align: center;
    padding: 10px 0;
    background: #344052;
    border-top: 1px solid #101010;
    display: none
}

.hide-person-operate>li {
    overflow: hidden;
    width: 100%;
    height: 30px;
    line-height: 30px;
    margin: 0;
    padding: 0;
    min-width: 20px;
    padding: 0px 15px;
}

.hide-person-operate>li>a {
    width: 100%;
    height: 30px;
    line-height: 30px;
    margin: 0;
    padding: 0;
    color: #eee
}

.hide-person-operate li:hover {
    background: #434e62;
}

.hide-person-operate li:hover a {
    padding: 0px;
    margin: 0px;
    color: #fff;
}


/*左侧导航栏样式*/

#side-menu a {
    border-left: 4px solid #344052;
}

#side-menu li a:hover {
    border-left: 4px solid #58b7f7;
    color: #fff;
    outline: 0;
    background-color: #434e62!important;
}

#side-menu .nav-second-level li a:hover {
    border-left: 4px solid #58b7f7;
    color: #fff;
    outline: 0;
    background-color: #434e62!important;
}

#side-menu .cur {
    border-left: 4px solid #58b7f7;
    background: #434e62 !important;
}


/*字体不加粗*/

#side-menu * {
    font-weight: normal;
    font-size: 14px;
}


/*调整二级导航栏行间距*/

.nav-second-level li a {
    padding: 10px 15px 10px 10px;
    padding-left: 52px;
}


/*tab页签部分*/

.page-tabs a.active {
    /*background: #59b6fc;*/
    background: #434e62;
    color: #fff;
}

.page-tabs a:hover {
    /*background: #59b6fc;*/
    background: #434e62;
    color: #fff;
}

.page-tabs .active {
    /*background: #59b6fc !important;*/
    background: #434e62 !important;
    border-left: 0;
}


/*消息（小铃铛）样式*/

.news-parent {
    position: relative;
}

#nuReadMsg {
    /*display: inline-block;*/
    position: absolute;
    top: 11px;
    right: 3px;
    width: 18px;
    height: 18px;
    padding: 0 2px;
    line-height: 18px;
    text-align: center;
    background-color: #f62020;
    border-radius: 50%;
    -moz-font-size: 8px;
    -webkit-font-size: 8px;
    font-size: 8px;
    -webkit-text-size-adjust: none;
    color: #fff;
    font-weight: 400;
    display: none;
}

.fa-home {
    font-size: 15px !important;
}


/*隐藏菜单*/

.menu-bar {
    width: 35px;
    height: 30px;
    /*border: 1px solid #333;
        background: #344052;*/
    margin-top: 12px;
    display: none;
}

.navbar-toggle.menu-bar {
    /*color: #fff;*/
}

.menu-bar span {
    /*border: 1px solid #fff;*/
    margin-left: -6px;
}

#menuNav.navbar-static-side {
    display: block;
}


/*媒体查询，使其尽量可以适应手机*/

@media (max-width: 900px) {
    .currentEnterpriseLi span {
        display: none
    }
}

@media (max-width: 768px) {
    .navbar-header {
        width: 100% !important;
    }
    .navbar-right {
        clear: both;
        width: 100% !important;
        margin-left: 20px;
    }
    .menu-bar {
        display: block;
    }
    #menuNav.navbar-static-side {
        display: none;
        width: 100%;
        z-index: 2001;
        padding-top: 120px;
        height: auto;
    }
    body.full-height-layout #page-wrapper {
        height: calc(100% - 121px);
        margin: 121px 0 0 0px;
    }
    #tabTab {
        display: none;
    }
    #content-main {
        height: 100%;
    }
}

@media (max-width: 500px) {
    .navbar-header {
        white-space: nowrap;
    }
    .person-operate {
        overflow: hidden;
    }
}