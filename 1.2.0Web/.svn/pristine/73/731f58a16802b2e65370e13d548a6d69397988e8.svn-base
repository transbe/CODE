/*第一行tab*/
.first-box{
    min-width: 400px;
}

.item {
    border: 1px solid #e3e3e3;
    cursor: pointer;
    padding: 0;
}

.item:first-child {
    border-right: none;
    border-radius: 16px 0 0 16px;
}

.item.active {
    border-color: #58b7f7;
    background: #edf7ff;
    color: #58b7f7;
}

.item:hover {
    border-color: #58b7f7;
    background: #edf7ff;
    color: #58b7f7;
    outline: 0;
}


/*设置tab文字*/

.btn_wrapper {
    padding: 0;
    text-align: center;
    color: #999;
}

.form-horizontal .row.row-content:first-of-type {
    margin-bottom: -5px;
}

/*媒体查询*/

@media screen and (max-width:1900px) {
    .form-horizontal .row-content:first-of-type .row-lab {
        flex-basis: 60%;
        margin-bottom: 5px;
    }
    .form-horizontal .row-content:first-of-type .row-lab:nth-of-type(2) {
        margin-left: 0px;
    }
    .form-horizontal .row-content:last-of-type .row-lab {
        flex-basis: 40%;
        margin-bottom: 5px;
    }
}