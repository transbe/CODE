ul,
li {
    list-style: none;
    padding: 0;
    margin: 0;
}

a {
    text-decoration: none;
    letter-spacing: 2px;
}

a:hover {
    text-decoration: none
}

.middle {
    vertical-align: middle !important;
}

.tb-th-bg {
    background-color: #e9e8e6;
    text-align: center;
    color: #333
}

.tb-td-bg {
    background-color: #f7f7f7;
    /*white-space: nowrap*/
}

td ul li {
    display: inline-block;
    text-align: center;
}

td ul li a {
    display: block;
    width: 100%;
    height: 100%;
}


/*td ul li a img {}*/

ul li span {
    display: block;
    width: 100%;
    text-align: center
}

.tb-border {
    border: 1px solid #ddd;
}

.tb-border td {
    border: 1px solid #ddd;
    text-align: center
}

.view-detail-table {
    width: 100% !important;
    /*table-layout: fixed !important;*/
}

.text-right {
    text-align: right !important;
}

.tb-null-line {
    padding: 3px !important;
    margin: 0 !important;
    border-right: 1px solid #fff !important;
    border-left: 1px solid #fff !important;
}


