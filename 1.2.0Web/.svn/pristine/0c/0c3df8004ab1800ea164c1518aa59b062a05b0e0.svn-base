                                    <div class="feedback_img_list left"></div>
                                    <div class="feedback_img_file">
                                        <input type="file" onchange="handleFiles(this);" class="upload_picture" name="file" multiple="" />
                                    </div>
                                    <div class="feedback_img_add left">
                                        <span class="addImg">添加图片</span>
                                    </div>
