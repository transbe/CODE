   /*通用样式*/
   
   .flex-box {
       display: -webkit-flex;
       display: flex;
       flex-wrap: nowrap;
   }
   
   .container {
       padding: 10px;
   }
   
   .welcom-box {
       padding: 8px;
   }
   
   .header_box {
       width: 100%;
       padding: 0 8px;
       background: #fff;
       border-radius: 4px 4px 0 0;
       border-bottom: 1px solid #eee;
   }
   
   .welcom-header {
       width: 100%;
       line-height: 40px;
       padding: 10px 0;
       flex-wrap: nowrap;
       justify-content: space-between;
   }
   
   .header-model {
       flex-basis: 24%;
       border-radius: 4px;
       padding: 0 10px;
       color: #fff;
       background: #59b6fc;
   }
   
   .header-title {
       flex-basis: 40%;
       white-space: nowrap;
       text-align: center;
       font-size: 16px;
   }
   
   .header-content {
       flex-basis: 60%;
       text-align: center;
       font-size: 16px;
       white-space: nowrap
   }
   /*title样式*/
   
   .common-title {
       width: 100%;
       height: 40px;
       line-height: 40px;
       color: #333;
   }
   
   .title-left {
       flex-basis: 80%;
       text-align: left;
       /*border: 1px solid pink;*/
   }
   
   .title-right {
       flex-basis: 20%;
       /*border: 1px solid blue;*/
       text-align: right
   }
   
   .title-style {
       font-size: 16px;
   }
   
   .left-bg::before {
       display: inline-block;
       width: 5px;
       height: 25px;
       content: "";
       background: #f79324;
       margin-right: 5px;
       vertical-align: middle
   }
   
   .right-arrows {
       font-size: 12px;
       color: #59b6fc;
   }
   
   .right-arrows::after {
       display: inline-block;
       content: ">";
       color: #59b6fc;
       font-size: 12px;
       transform: scaleY(1.5);
       margin-left: 5px;
   }
   /*执行中样式*/
   
   .execute-task {
       min-height: 550px;
       border-radius: 4px;
       background: #fff;
       padding: 8px;
       margin: 0;
   }
   
   .execute-main {
       width: 100%;
       min-height: 500px;
       display: -webkit-flex;
       display: flex;
       flex-wrap: wrap;
   }
   
   .execute-graph-box {
       flex-basis: 48%;
       min-height: 250px;
       padding: 5px;
       height: 100%;
       overflow: hidden;
   }
   
   .execute-graph-box:first-of-type {
       margin-right: 2%;
   }
   
   .execute-graph-box:last-of-type {
       flex-basis: 98%;
   }
   
   .execute-graph-box .common-title {
       height: 15%;
   }
   
   .execute-graph-box .title-style {
       font-size: 16px;
   }
   
   .execute-graph-box .execute-graph {
       min-height: 210px;
       /*border: 1px solid red;*/
   }
   
   .execute-graph-box:first-of-type .execute-graph {
       border-right: 1px solid #eee
   }
   
   #executeModel {
       overflow: auto;
       text-align: left;
       min-height: 210px;
       max-height: 400px;
       /*border: 1px solid pink;*/
   }
   
   .whole-model {
       width: 19%;
       margin: 0 0.3%;
       min-height: 210px;
       display: inline-block;
       text-align: center;
       overflow: visible;
       /*border: 1px solid yellow*/
   }