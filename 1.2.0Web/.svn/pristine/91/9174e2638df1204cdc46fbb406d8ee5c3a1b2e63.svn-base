* {
    margin: 0px;
    padding: 0px
}

#page-wrapper {
    background: #edf1f2
}

#page-wrapper .container-fluid {
    padding-right: 15px;
    padding-left: 15px
}

#page-wrapper .content {
   margin-top: 7px;
   margin-left: -7px;
}

#page-wrapper .content #toolbar .export-all,
#page-wrapper .content #toolbar .inviteuser {
    margin: 0 10px;
    color: #53b5fc;
    font-size: 14px;
    cursor: pointer
}

#page-wrapper .content .pull-right button {
    color: #59b6fc
}

#page-wrapper table td {
    white-space: nowrap;
    width: 16%;
    text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    overflow: hidden
}

.remove {
    background: #59b6fc;
    color: #fff;
    border: 1px solid transparent;
    border-radius: 4px;
    padding: 0px 5px
}

.disabledremove {
    color: #fff;
    border: 1px solid transparent;
    border-radius: 4px;
    padding: 0px 5px
}

.fixed-table-body {
    border: 1px solid #ddd
}

.fixed-table-container {
    border: none
}

.bootstrap-table {
    margin-bottom: 20px
}

.search_wrapper {
    margin-right: 0px !important
}

.table {
    border-bottom: none !important
}