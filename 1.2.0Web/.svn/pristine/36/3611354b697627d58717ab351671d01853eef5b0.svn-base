/*表单内容*/
.form_main{
    display: block;
}
.form_title{
    height: 70px;
}
.form_title p{
    font-size: 18px;
    color: #666;
    text-align: center;
    line-height: 70px;
}
.form_list{
    overflow: hidden;
    height: 60px;
}
.form_list.list_address{
    height: 50px;
}
.form_list_fl{
    width: 170px;
    text-align: right;
}
.form_list_fl span{
    font-size: 16px;
    color: #666;
    line-height: 40px;
}
.form_list_fr{
    width: 545px;
}
.form_list_fr input[type="text"]{
    width: 470px;
    height: 38px;
    line-height: 38px;
    padding-left: 10px;
    border: 1px solid #dddddd;
}
.form_list_fr select{
    width: 230px;
    height: 38px;
    padding-left: 10px;
    border: 1px solid #dddddd;
}
.form_list_fr input[type="text"].form_verification{
    width: 230px;
    float: left;
}
.form_list_fr p{
    font-size: 12px;
    color: #ff0000;
    font-family: "宋体";
    line-height: 14px;
    padding-top: 1px;
    display: none;
}
.form_list_main{
    overflow: hidden;
}
.form_list_main span.get_verification{
    display: inline-block;
    *display: inline;
    *zoom: 1;
    width: 230px;
    height: 38px;
    line-height: 38px;
    border: 1px solid #3496df;
    color: #59b6fc;
    font-size: 20px;
    border-radius: 5px;
    text-align: center;
    cursor: pointer;
    background: #fff;
    float: left;
    margin-left: 8px;
}
.form_list_main span.get_verification_no{
    display: inline-block;
    *display: inline;
    *zoom: 1;
    width: 230px;
    height: 38px;
    line-height: 38px;
    border: 1px solid #3496df;
    color: #59b6fc;
    font-size: 20px;
    border-radius: 5px;
    text-align: center;
    cursor: pointer;
    background: #fff;
    float: left;
    margin-left: 8px;
}
.form_btn{
    padding-top: 20px;
    text-align: center;
    height: 50px;
}
.form_btn span{
    display: inline-block;
    *display: inline;
    *zoom: 1;
    width: 180px;
    height: 48px;
    line-height: 48px;
    border: 1px solid #000000;
    color: #ffffff;
    font-size: 20px;
    margin: 0 20px;
    border-radius: 5px;
    cursor: pointer;
}
.form_btn span.cancel_btn{
    background: #fcc159;
    border-color: #dfa031;
}
.form_btn span.submit_btn{
    background: #59b6fc;
    border-color: #3496df;
}

.wicon{
    background-image: url("data:image/png;base64,");
    background-repeat:no-repeat;
    background-position:right center;
}

/*提交成功*/
.form_success{
    display: none;
}
.success_img{
    padding-top: 100px;
    text-align: center;
}
.success_text{
    padding-top: 30px;
}
.success_text p{
    font-size: 20px;
    color: #999;
    line-height: 40px;
    text-align: center;
}
.success_btn{
    padding-top: 120px;
    text-align: center;
}
.success_btn span{
    display: inline-block;
    *display: inline;
    *zoom: 1;
    width: 300px;
    height: 48px;
    line-height: 48px;
    border: 1px solid #3496df;
    color: #ffffff;
    font-size: 20px;
    margin: 0 20px;
    border-radius: 5px;
    cursor: pointer;
    background: #59b6fc;
}