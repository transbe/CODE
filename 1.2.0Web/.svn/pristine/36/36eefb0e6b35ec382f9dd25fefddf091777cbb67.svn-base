// 查看申请
function viewApplication(reportID) {
	var eventID = "";
	var rows = $("#applictionTable").bootstrapTable('getSelections');
	if (reportID != null && reportID != "" && reportID != undefined) {
		eventID = reportID;
	} else if (rows.length != 1) {
		layer.confirm('请选择一条数据', {
			btn: ['确定'],
			skin: 'self'
		});
		return;
	} else {
		eventID = rows[0].objectId;
	}
	var title = "";
	if (reportType == 1) {
		title = "查看阴保有效性报告申请";
	} else {
		title = "查看阴保完整性报告申请";
	}
	var index = parent.layer.open({
		type: 2, //0（信息框，默认）1（页面层）2（iframe层）3（加载层）4（tips层）
		title: title,
		area: ['950px', "600px"],
		btn: ['关闭'],
		content: 'src/html/data_analysis/report/view_application.html?reportType=' + reportType + '&objectId=' + eventID
	});
}

/**
 * @desc 查看报告
 * 		 1、只能在【提交报告/修订报告/验收通过】状态下
 * 		 2、只有在【提交报告】状态下才有【接收】【退回】按钮
 * @param {*String} objectId 
 * @param {*String} applyStatus 
 */
function viewReport(objectId, applyStatus) {
	var eventID = "";
	var rows = $('#applictionTable').bootstrapTable('getSelections');
	if (objectId != "" && objectId != undefined && objectId != null) {
		eventID = objectId;
	} else if (rows.length == 1) {
		eventID = rows[0].objectId;
		applyStatus = rows[0].applyStatus;
	} else {
		layer.confirm('请选中一条记录！', {
			btn: ['确定'], //按钮
			skin: 'self'
		});
		return;
	}
	if (applyStatus != "验收通过" && applyStatus != "提交报告" && applyStatus != "修订报告") {
		layer.confirm('没有报告可以查看！', {
			btn: ['确定'], //按钮
			skin: 'self'
		});
		return;
	}
	$("#viewReport").attr('disabled', true);
	// 设置窗口标题
	var title = "";
	if (reportType == 1) {
		title = "查看阴保有效性报告";
	} else {
		title = "查看阴保完整性报告";
	}
	// 设置窗口按钮
	if (applyStatus = "提交报告") {
		var index = parent.layer.open({
			type: 2, //0（信息框，默认）1（页面层）2（iframe层）3（加载层）4（tips层）
			title: title,
			area: ['800px', '600px'],
			btn: ['接收', '退回', '关闭'],
			yes: function (index, layero) {
				var windowObj = layero.find('iframe')[0].contentWindow;
				var flag = windowObj.changeReportStatus(5);
				if (flag) {
					parent.layer.close(index);
				}
			},
			btn2: function (index, layero) {
				var windowObj = layero.find('iframe')[0].contentWindow;
				var flag = windowObj.changeReportStatus(4);
				if (!flag) {
					return flag;
				}
			},
			end: function (index, layero) {
				$("#viewReport").attr('disabled', false);
			},
			content: ["src/html/data_analysis/report/view_report.html?objectId=" + eventID, 'no']
		});
	}else{
		var index = parent.layer.open({
			type: 2, //0（信息框，默认）1（页面层）2（iframe层）3（加载层）4（tips层）
			title: title,
			area: ['800px', '600px'],
			btn: ['关闭'],
			end: function (index, layero) {
				$("#viewReport").attr('disabled', false);
			},
			content: ["src/html/data_analysis/report/view_report.html?objectId=" + eventID, 'no']
		});		
	}
	document.getElementById('viewReport').disabled = false;
	zhugeMess("查看", '');
}


// 诸葛IO
function zhugeMess(params, param) {
	var zhugeName = "";
	if (reportType == 1) {
		zhugeName = params + "阴保有效性报告";
	} else {
		zhugeName = params + "阴保完整性报告";
	}
	//诸葛IO
	try {
		if (zhugeSwitch == 1) {
			zhuge.track(zhugeName, {
				'结果': param
			});
		}
	} catch (error) {}
}